<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mypet.mapper.TestMapper">
	
	<insert id="test1">
		insert into test1 values(test1_seq.nextval)
	</insert>
	
	<insert id="test2">
		insert into test2 values(test1_seq.currval)
	</insert>

	<insert id="test3">
		insert into test3 values(${num})
	</insert>	
	

</mapper>